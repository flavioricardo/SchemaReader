{
  "name": "Schema Reader",
  "version": "1.0",
  "description": "Identify and describe microdata contents on a webpage.",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["library/bootstrap/css/bootstrap.min.css", "library/bootstrap/css/bootstrap-responsive.min.css", "library/css/style.css"],
      "js": ["library/js/jquery-1.8.3.min.js", "library/js/jquery.tmpl.min.js", "library/bootstrap/js/bootstrap.min.js", "library/js/schemaReader.js"]
    }
  ],
  "icons" : {
    "48" : "library/images/icon-48.png",
    "128" : "library/images/icon-128.png"
  },
  "web_accessible_resources": [
    "library/bootstrap/img/glyphicons-halflings.png",
    "library/images/icon-19.png"
  ],
  "manifest_version": 2
}